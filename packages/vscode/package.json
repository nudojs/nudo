{
  "name": "nudo-vscode",
  "displayName": "Nudo",
  "description": "Nudo type inference engine for VS Code / Cursor",
  "version": "0.0.1",
  "private": true,
  "publisher": "nudo",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nudo.selectCase",
        "title": "Nudo: Select Case"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0"
  },
  "scripts": {
    "build": "tsup src/extension.ts --format cjs --outDir out --external vscode",
    "package": "npx @vscode/vsce package --no-dependencies"
  }
}
